Int[] x[];
Int[] y[];

x[] = ReadStream;
y[] = ReadStream;

Int a;
Int b;

a = 1;
b = 1;

while(a <= sizeOf(x[])){

    if(1==sizeOf(x[])){
        output(x[a]);
        b+=3;
    }

    if( a+1 <= sizeOf(x[])){
        output(x[a]);
        output(x[a+1]);
    }
    if( b <= sizeOf(y[])){
        output(y[b]);
    }
    if( b+1 <= sizeOf(y[])){
        output(y[b+1]);
    }
    if( b+2 <= sizeOf(y[])){
        output(y[b+2]);
    }

    a+=2;
    b+=3;
}